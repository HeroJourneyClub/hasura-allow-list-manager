{
  "name": "@herojourneyclub/hasura-allow-list-manager",
  "version": "1.0.6",
  "description": "Populate the Hasura allow-list from found GraphQL operations in your project",
  "main": "dist/index.js",
  "bin": {
    "hasura-allow-list-manager": "dist/start.js"
  },
  "scripts": {
    "preglobal:add": "npm run build",
    "build": "tsc -p .",
    "dev": "ts-node-dev --no-notify --transpile-only src/start.ts",
    "lint": "eslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "postpack": "shx rm -rf dist",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "keywords": [
    "Hasura",
    "GraphQL"
  ],
  "maintainers": [
    "Hero Journey Club <devs@herojourney.club>"
  ],
  "engines": {
    "node": "^16 || ^18"
  },
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/HeroJourneyClub/hasura-allow-list-manager/issues"
  },
  "homepage": "https://github.com/HeroJourneyClub/hasura-allow-list-manager#README.md",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.16",
    "@graphql-tools/load": "^7.8.12",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/utils": "^9.2.1",
    "axios": "^0.21.4",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "diff": "^5.1.0",
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@types/diff": "^5.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "delay": "^5.0.0",
    "docker-compose": "^0.23.12",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-watch": "^7.0.0",
    "jest": "^27.0.5",
    "prettier": "^2.3.1",
    "shx": "^0.3.2",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist/*",
    "!dist/__test__",
    "!dist/**/*.test.*",
    "LICENSE",
    "NOTICE",
    "README.md"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  }
}
